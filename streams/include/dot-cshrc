if ($?CLASSPATH) then
else
  setenv CLASSPATH .
endif

# location of java and jikes
setenv JAVA_HOME /usr/uns/jdk1.5.0_01
setenv JIKES_HOME /home/bits8/streamit/jikes-1.22-customized

# java classes
setenv CLASSPATH ${CLASSPATH}:${JAVA_HOME}/jre/lib/rt.jar
## antlr classes
setenv CLASSPATH ${CLASSPATH}:/usr/uns/java/antlr.jar
# streamit classes
setenv CLASSPATH ${CLASSPATH}:${STREAMIT_HOME}/src
setenv CLASSPATH ${CLASSPATH}:${STREAMIT_HOME}/3rdparty
setenv CLASSPATH ${CLASSPATH}:${STREAMIT_HOME}/3rdparty/cplex/cplex.jar
setenv CLASSPATH ${CLASSPATH}:${STREAMIT_HOME}/3rdparty/jgraph/jgraph.jar
setenv CLASSPATH ${CLASSPATH}:${STREAMIT_HOME}/3rdparty/JFlex/jflex.jar

# for running dotty
setenv LEFTYPATH /home/bits7/NO_BACKUP/streamit/dotty

# the current version of starsearch we're using
setenv TOPDIR ${STREAMIT_HOME}/misc/raw

# the path to ATLAS
setenv ATLAS_HOME /home/bits7/NO_BACKUP/streamit/ATLAS

# add STREAMIT_HOME to the path in order to find "knit"
if ($?PATH) then
  setenv PATH ${PATH}:${STREAMIT_HOME}
else
  setenv PATH ${STREAMIT_HOME}
endif

# some libraries needed for jikes-1.22
if ($?LD_LIBRARY_PATH) then
  setenv LD_LIBRARY_PATH ${LD_LIBRARY_PATH}:/home/streamit/lib
else
  setenv LD_LIBRARY_PATH /home/streamit/lib
endif

# add java and jikes to path
setenv PATH ${JAVA_HOME}/bin:${JIKES_HOME}/bin:${PATH}

# Perl on RHEL-3
if ($?PERL5LIB) then
  setenv PERL5LIB /usr/uns/encap/perl-5.8.0/lib/5.8.0/:${PERL5LIB}
else
  setenv PERL5LIB /usr/uns/encap/perl-5.8.0/lib/5.8.0/
endif

# include extra LaTeX packages
# (Note that this *always* DTRT: the TeX path-searching stuff interprets
# a blank entry as "the default path".)
if ($?TEXINPUTS) then
  setenv TEXINPUTS ${STREAMIT_HOME}/include/tex:${TEXINPUTS}
else
  setenv TEXINPUTS ${STREAMIT_HOME}/include/tex:
endif


if ($?CLASSPATH) then
else
  setenv CLASSPATH .
endif

# java classes
setenv CLASSPATH ${CLASSPATH}:/usr/uns/jdk1.3.1_01/jre/lib/rt.jar
## antlr classes
setenv CLASSPATH ${CLASSPATH}:/usr/uns/java/antlr.jar
# streamit classes
setenv CLASSPATH ${CLASSPATH}:${STREAMIT_HOME}/compiler
setenv CLASSPATH ${CLASSPATH}:${STREAMIT_HOME}/compiler/3rdparty
setenv CLASSPATH ${CLASSPATH}:${STREAMIT_HOME}/compiler/3rdparty/cplex/cplex.jar
setenv CLASSPATH ${CLASSPATH}:${STREAMIT_HOME}/library/java

# for running dotty
setenv LEFTYPATH /home/bits7/NO_BACKUP/streamit/dotty

# the current version of starsearch we're using
setenv STARSEARCH_HOME /home/bits7/NO_BACKUP/streamit/starsearch
setenv TOPDIR ${STARSEARCH_HOME}

# the path to ATLAS
setenv ATLAS_HOME /home/bits7/NO_BACKUP/streamit/ATLAS

# add STREAMIT_HOME to the path in order to find "knit"
if ($?PATH) then
  setenv PATH ${PATH}:${STREAMIT_HOME}
else
  setenv PATH ${STREAMIT_HOME}
endif

# include extra LaTeX packages
# (Note that this *always* DTRT: the TeX path-searching stuff interprets
# a blank entry as "the default path".)
if ($?TEXINPUTS) then
  setenv TEXINPUTS ${STREAMIT_HOME}/include/tex:${TEXINPUTS}
else
  setenv TEXINPUTS ${STREAMIT_HOME}/include/tex:
endif

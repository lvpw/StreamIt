#setenv CLASSPATH ${CLASSPATH}:${PRE}swt.motif${VER}/ws/motif/swt.jar
#setenv CLASSPATH ${CLASSPATH}:${PRE}ui.views${VER}/views.jar
#setenv CLASSPATH ${CLASSPATH}:${PRE}core.runtime${VER}/runtime.jar

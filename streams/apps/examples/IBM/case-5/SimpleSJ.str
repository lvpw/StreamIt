void->void pipeline SimpleSJ {
    add IntSource();
    add splitjoin {
	split duplicate;
	add F();
	add F();
	join roundrobin;
    }
    add IntPrinter();
}

int->int filter F {
    work push 1 pop 1 {
	push(pop());
    }
}

void->int filter IntSource {
    int x;
    init {
        x = 0;
    }
    work push 1 {
        push(x++);
    }
}

int->void filter IntPrinter {
    work pop 1 {
        println(pop());
    }
}
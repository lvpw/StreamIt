/*
 * HelloWorld6.str: a serial integer printer
 * $Id: HelloWorld6.str,v 1.5 2002-06-12 20:31:04 dmaze Exp $
 */

void->int filter IntSource {
    int x;
    init {
        x = 0;
    }
    work push 1 {
        push(x++);
    }
}

int->void filter IntPrinter {
    work pop 1 {
        print(pop());
    }
}

void->void pipeline HelloWorld6 {
    add IntSource();
    add IntPrinter();
}

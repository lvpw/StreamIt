/*
 * HelloWorld6.str: a serial integer printer
 * $Id: HelloWorld6.str,v 1.6 2005-11-04 01:06:17 dimock Exp $
 */

void->int filter IntSource {
    int x;
    init {
        x = 0;
    }
    work push 1 {
        push(x++);
    }
}

int->void filter IntPrinter {
    work pop 1 {
        println(pop());
    }
}

void->void pipeline HelloWorld6 {
    add IntSource();
    add IntPrinter();
}

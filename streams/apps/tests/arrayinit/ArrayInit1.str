/**
 * This tests array initializers as fields.
 **/

void->void pipeline ArrayInit1 {
    add Source();
    add Tester;
    add Printer();
}

int->int filter Tester() {
    int[3] arr = {1, 2, 3};

    work pop 3 push 3 {
        push(arr[0] * pop());
        push(arr[1] * pop());
        push(arr[2] * pop());
    }
}

void->int stateful filter Source {
    int i = 0;
    work push 1 { push(i++); }
}

int->void filter Printer {
    work pop 1 { println(pop()); }
}

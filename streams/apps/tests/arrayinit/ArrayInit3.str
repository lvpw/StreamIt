/**
 * This tests array initializers as local variables.
 **/

void->void pipeline ArrayInit3 {
    add Source();
    add Tester();
    add Printer();
}

int->int filter Tester() {
    work pop 3 push 3 {
        int[3] arr = {1, 2, 3};
        push(arr[0] * pop());
        push(arr[1] * pop());
        push(arr[2] * pop());
    }
}

void->int filter Source {
    int i = 0;
    work push 1 { push(i++); }
}

int->void filter Printer {
    work pop 1 { println(pop()); }
}

/**
 * This tests propagation of fields into array dimensions.
 */

void->void pipeline ArrayTest6 {
    add Source();
    add Test(10);
    add Printer();
}

int->int filter Test(int N) {
    // testing that N gets resolved into the type declaration of this
    // field
    int[N] arr;

    init {
	arr[0] = 1;
    }

    work pop 1 push 1 {
	push(arr[0] * pop());
    }
}

void->int stateful filter Source {
    int i = 0;
    work push 1 { push(i++); }
}

int->void filter Printer {
    work pop 1 { println(pop()); }
}

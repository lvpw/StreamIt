/**
 * This tests at the intersection of arrays and global prop.
 **/

static {
    int[2] A;

    init {
	A[0] = 2;
	A[1] = 2;
    }
}

void->void pipeline ArrayTest11 {
    add Source();
    // doesn't work
    add pipeline {
	add Test(A[1]);
    }
    // works
    //add Test(A[1]);
    add Printer();
}

int->int filter Test(int j) {
    work pop 1 push 1 {
	push (j * pop());
    }
}

void->int stateful filter Source {
    int i = 0;
    work push 1 { push(i++); }
}

int->void filter Printer {
    work pop 1 { println(pop()); }
}

/**
 * This tests assignment of arrays to each other.
 **/

void->void pipeline ArrayTest15 {
    add Source();
    add Test(10);
    add Printer();
}

int->int filter Test(int width) {
    int[width] A;
    int[width] B;

    work push 1 pop 1 {
	A = B;
	push(pop());
    }
}

void->int filter Source {
    int i = 0;
    work push 1 { push(i++); }
}

int->void filter Printer {
    work pop 1 { println(pop()); }
}

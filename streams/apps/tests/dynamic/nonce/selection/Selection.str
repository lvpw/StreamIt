void->void pipeline Selection {
    add FileReader<int>("./ints.in");
    add Select();
    add Process();
    add FileWriter<int>(stdout);
}

int->int filter Select() {
    work pop 1 push * {
        int x = pop();
        if (x < 1) {
            push(x);
        }
    }
}

int->int filter Process() {
    work pop 1 push 1 {
      push(pop());
    }
}


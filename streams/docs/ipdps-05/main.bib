@InProceedings{park99spdf1,
  author = {Chanik Park and Jaewoong Chung and Soonhoi Ha},
  title = "{Extended Synchronous Dataflow for Efficient DSP System Prototyping}",
  booktitle = "{10th IEEE Int. Workshop on Rapid System Prototyping}",
  year = {1999},
  mynotes = {
-------------------------------------------------------------------------------------------
- exactly the same motivation for messaging
- describes general solution with Global State Table (GST), tagged by iteration number
- but compiler arranges schedule to deliver directly, so only overhead is checking for
  right iteration to deliver messages (like us)
- less formal description of schedule constraints
- calls extended graph "Synchronous Piggybacked Dataflow Graph (PDSF)"
- does handle:
  - MPEG-1 (layer 3) audio decoder
     - sends parameters such as sampling freq, global/local gain, scale factor, etc.
- does not handle:
  - upstream messages
  - adjustable latencies
  - cyclo-static delivery (introduces more complexity -- can't just 
    check periodic "offset" in receiver, need to check SDEP)
-------------------------------------------------------------------------------------------
}
}


text('black',80,320,'Courier',0,10,27,0,0,1,300,323,0,0,9,2,0,1,0,0,0,2,0,0,0,0,"",0,0,0,[
	"class FIRFilter extends Filter {",
	"   float[] weights;",
	"   int N;",
	"",
	"   void init(float[] weights) {",
	"      setInput(Float.TYPE); setOutput(Float.TYPE);",
	"      setPush(N); setPop(1); setPeek(N); ",
	"      this.weights = weights;",
	"      this.N = weights.length;",
	"   }",
	"",
	"   void work() {",
	"      float sum = 0;",
	"      for (int i=0; i<N; i++) ",
	"         sum += input.peek(i)*weights[i];",
	"      input.pop();",
	"      output.push(sum);",
	"   }",
	"}",
	"",
	"class Main extends Pipeline {",
	"   void init() {",
	"      add(new DataSource());",
	"      add(new FIRFilter(N));",
	"      add(new Display());",
	"   }",
	"}"]).
